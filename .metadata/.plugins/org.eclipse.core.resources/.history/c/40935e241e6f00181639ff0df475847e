package kugui.owd.privateMessenger;

import java.lang.Thread;



public class RxThread extends Thread{
	
	public void run() {
		
		try {
			while(true) {
				Thread.sleep(10000);
				System.out.println("please let me see the good dream...");
			}
		}catch(Exception ex) {
			ex.printStackTrace();
		}
	}
	
}